<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc; *.js" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="LiveEditOptions">
    <option name="elementHighlighting" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
    <pattern icon="important" useCustomColors="true" case-sensitive="true" pattern="\bIMPORTANT\b.*">
      <option name="FOREGROUND" value="ff4855" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
  </component>
</application>